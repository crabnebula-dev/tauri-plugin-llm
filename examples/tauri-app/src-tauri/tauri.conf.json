{
  "productName": "tauri-app",
  "version": "0.1.0",
  "identifier": "dev.crabnebula.plugin-llm-example",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": false,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "tauri-app",
        "width": 800
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "llm": {
      "llmconfig": {
        "name": "Mock",
        "tokenizer_file": "",
        "model_config_file": "",
        "model_index_file": "",
        "model_file": "",
        "model_dir": "",
        "penalty": 1.0,
        "seed": "Random",
        "sampling_config": "TopK",
        "verbose": true
      }
    }
  }
}
